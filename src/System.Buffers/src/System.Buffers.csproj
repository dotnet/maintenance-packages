<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;$(NetFrameworkMinimum)</TargetFrameworks>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <StrongNameKeyId>Open</StrongNameKeyId>
    <AssemblyVersion>4.0.3.0</AssemblyVersion>
    <!-- Bump the below versions each time we need to service a new version and ensure IsPackable is set to true -->
    <IsPackable>true</IsPackable>
    <VersionPrefix>4.6.0</VersionPrefix>
    <PackageValidationBaselineVersion>4.5.1</PackageValidationBaselineVersion>
  </PropertyGroup>

</Project>
