<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <!-- Disabled warnings: One or more packages where restored using net461 TFM since NuGet couldn't find a netstandard2.0 supported asset in the package.
        NU1701 - 
     -->
