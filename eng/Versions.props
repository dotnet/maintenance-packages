    <MicrosoftDotNetBuildTasksTargetFrameworkVersion>8.0.0-beta.25105.2</MicrosoftDotNetBuildTasksTargetFrameworkVersion>
  </PropertyGroup>
  <!-- SDK dependencies -->
  <PropertyGroup>
      <MicrosoftDotNetApiCompatTaskVersion>10.0.100-preview.3.25124.13</MicrosoftDotNetApiCompatTaskVersion>
  </PropertyGroup>
</Project>
